{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "ProductInfo",
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "price": {
        "type": "number"
      },
      "totalQuantity": {
        "type": "number"
      },
      "sellerId": {
        "type": "integer"
      },
      "categoryId": {
        "type": "integer"
      },
      "discount": {
        "type": "number"
      },
      "description": {
        "type": "string"
      },
      "address": {
        "type": "string"
      },
      "productStatusId": {
        "type": "integer"
      },
      "shippingMethodId": {
        "type": "integer"
      },
      "createdAt": {
        "type": "string",
        "format": "date-time"
      },
      "updatedAt": {
        "type": "string",
        "format": "date-time"
      }
    },
    "required": [
      "name",
      "price",
      "totalQuantity",
      "sellerId",
      "categoryId"
    ],
    "additionalProperties": false
}
