{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "main": "bin/app.ts",
  "scripts": {
    "lint": "eslint \"./src/**/*.{ts,tsx,json}\"",
    "lint:fix": "eslint --fix . --ext .ts",
    "prettier": "npx prettier './**/*.{ts,tsx,json,css,scss,md}' --check",
    "prettier:fix": "prettier --write './**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "format": "npm run prettier:fix && npm run lint:fix",
    "compile": "tsc && tsc-alias",
    "tsc": "tsc",
    "build": "npm run compile && cdk synth",
    "deploy": "npm run tsc && npm run build_node_layer && cdk deploy ${1} --outputs-file ./cdk-outputs.json --exclusively --require-approval never",
    "deploy_windows": "npm run tsc && npm run build_node_layer_windows && cdk deploy ${1} --outputs-file ./cdk-outputs.json --exclusively --require-approval never",
    "deploy-all": "npm run tsc && npm run build_node_layer  && cdk deploy --all --outputs-file ./cdk-outputs.json --exclusively",
    "test": "npm run compile && mocha dist/tests/**/*test.js",
    "test:coverage": "npm run compile && nyc nyc --reporter=html mocha dist/tests/**/*test.js",
    "build_node_layer": "mkdir -p ./dist/src/layer/nodejs &&  cp ./package.json ./dist/src/layer/nodejs/package.json && npm --prefix ./dist/src/layer/nodejs/ install ./dist/src/layer/nodejs/ --omit=dev",
    "build_node_layer_windows": "if not exist ./dist/src/layer/nodejs mkdir .\\dist\\src\\layer\\nodejs && copy .\\package.json .\\dist\\src\\layer\\nodejs\\package.json && npm --prefix .\\dist\\src\\layer\\nodejs\\ install .\\dist\\src\\layer\\nodejs\\ --omit=dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/express": "^4.17.16",
    "@types/mocha": "^10.0.1",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.14.6",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "aws-cdk-lib": "^2.64.0",
    "cdk": "^2.66.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "constructs": "^10.0.0",
    "eslint": "^8.32.0",
    "eslint-plugin-react": "^7.32.1",
    "express": "^4.18.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.3",
    "sinon": "^15.0.1",
    "source-map-support": "^0.5.21",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "aws-sdk": "^2.1336.0",
    "mysql": "^2.18.1"
  },
  "nyc": {
    "branches": "&gt;80",
    "lines": "&gt;80",
    "functions": "&gt;80",
    "statements": "&gt;80",
    "extension": [
      ".ts"
    ],
    "exclude": [
      "lib",
      "bin",
      "coverage",
      "stacks",
      "tests/**",
      "config"
    ],
    "all": true,
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  }
}
